import pandas as pd
from bs4 import BeautifulSoup

# Load the HTML file
file_path = 'C:\Aaditya_projects\work\emailfir\htmlcodetable.html'
with open(file_path, 'r') as file:
    html_content = file.read()

# Parse the HTML content
soup = BeautifulSoup(html_content, 'html.parser')

# Find the table
table = soup.find('table')

# Extract rows from the table
rows = table.find_all('tr')

# Function to process the table data
def process_table_data(rows):
    extracted_data = {}
    current_keys = []

    for row in rows:
        cells = row.find_all('td')
        row_data = [cell.get_text(strip=True) for cell in cells]

        if len(row_data) == 1:
            continue  # Ignore rows with only one cell
        elif len(row_data) == 2:
            key, value = row_data
            extracted_data[key] = value
        elif len(row_data) == 3:
            if not current_keys:
                current_keys = row_data
            else:
                row_dict = dict(zip(current_keys, row_data))
                extracted_data.update(row_dict)

    return extracted_data

# Process the table data
processed_data = process_table_data(rows)

# Print the processed data
for key, value in processed_data.items():
    print(f"{key}: {value}")
